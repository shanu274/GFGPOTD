class Solution
{
    //Function to find position of first set bit in the given number.
    public static int getFirstSetBit(int n){
            
        // Your code here
            int count = 0;
            if(n == 0){
                return 0;
            }
            
            while(n > 0){
                count += 1;
                if(n%2 == 1){
                    return count;
                }else{
                    n = n / 2;
                }
            }
            
            return 0;
    }
}
